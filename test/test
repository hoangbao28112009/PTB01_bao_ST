'''
t = int(input("loại tài liệu:"))
p = int(input("số trang:"))
c = int(input("số bản cần in:"))
if (t == 1):
   print("số giấy cần dùng:",p * c )
elif (t == 2):
   print("số giấy cần dùng:",p * 2 * c )
